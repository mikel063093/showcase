{% extends "plantilla/principal.html.twig" %}

{% block tituloLocal %}Estadisticas{% endblock %}

{% block btnAdicionar  %}

{% endblock %}

{% block js %}
    <script type="text/javascript">
    $(document).ready(function(){

         $.ajax({
            type: "POST",
            url: '{{path("verEstadisticas")}}',
            dataType: "html",

            success: function(data) {
                $('#registros').html(data);
            }
        });
    });




    

    </script>
{% endblock %}