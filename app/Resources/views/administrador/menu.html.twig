<script>
    $(".mt a").click(function(evento){
        var opcion = $(this).attr('value');
        $(this).parent().parent().children().removeClass('active');
        $(this).parent().addClass('active');
       
        evento.preventDefault();
        $.ajax({
            url: opcion,
            type: 'POST',
            dataType: 'html',
            success:function(data){
                $('#contenido').html(data);
            }

        });
            
        
    });
</script>
